{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "label": "Basic onboarding",
    "Common": {
        "class": "Tenant",
        "hostname": "${local_host}.${domain_name}",
	    "myLicense": {
            "class": "License",
            "licenseType": "regKey",
            "regKey": "${regkey}"
        },
        "dbvars": {
            "class": "DbVariables",
            "ui.advisory.enabled": true,
            "ui.advisory.color": "${advisory_color}",
            "ui.advisory.text": "${advisory_text}"
        },
        "myDns": {
            "class": "DNS",
            "nameServers": [
                "${dns_server}",
                "2001:4860:4860::8844"
            ],
            "search": [
                "f5.com"
            ]
        },
        "myNtp": {
            "class": "NTP",
            "servers": [
                "${ntp_server}",
                "0.pool.ntp.org",
                "1.pool.ntp.org"
            ],
            "timezone": "${timezone}"
        },
        "myProvisioning": {
            "class": "Provision",
            "ltm": "${provision_ltm}",
            "avr": "${provision_avr}",
            "ilx": "${provision_ilx}",
            "asm": "${provision_asm}",
            "apm": "${provision_apm}"
        },
        "external": {
            "class": "VLAN",
            "tag": 1000,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.1",
                    "tagged": false
                }
            ]
        },
        "external-self": {
            "class": "SelfIp",
            "address": "${bigip_ext_priv_self_ip}/24",
            "vlan": "external",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "internal": {
            "class": "VLAN",
            "tag": 1003,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.4",
                    "tagged": false
                }
            ]
        },
        "internal-self": {
            "class": "SelfIp",
            "address": "${bigip_int_priv_self_ip}/24",
            "vlan": "internal",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },        
        "default": {
            "class": "Route",
            "gw": "${gateway}",
            "network": "default",
            "mtu": 1500
        },
        "app1Route": {
            "class": "Route",
            "gw": "${app1_net_gw}",
            "network": "${app1_net}",
            "mtu": 1500
        }
    }
}
