{
    "class": "Cloud_Failover",
    "environment": "aws",
    "controls": {
        "class": "Controls",
        "logLevel": "silly"
    },        
    "externalStorage": {
        "scopingTags": {
            "f5_cloud_failover_label": "paz-az-failover"
        }
    },
    "failoverAddresses": {
        "scopingTags": {
            "f5_cloud_failover_label": "paz-az-failover"
        }
    },
    "failoverRoutes": {
        "scopingTags": {
            "f5_cloud_failover_label": "gccap-az-failover"
        },
        "scopingAddressRanges": [
            {
                "range": "100.65.1.0/29",
                "nextHopAddresses": {
                    "discoveryType": "static",
                    "items": [
                        "10.1.2.11",
                        "10.1.12.11"
                    ]
                }
            },
            {
                "range": "0.0.0.0/0",
                "nextHopAddresses": {
                    "discoveryType": "static",
                    "items": [
                        "10.1.2.11",
                        "10.1.12.11"
                    ]
                }
            }
        ],
        "defaultNextHopAddresses": {
            "discoveryType": "static",
            "items": []
        }
    }
}