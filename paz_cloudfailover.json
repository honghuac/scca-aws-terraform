{
    "class": "Cloud_Failover",
    "environment": "aws",
    "externalStorage": {
        "scopingTags": {
            "f5_cloud_failover_label": "${cf_label}"
        }
    },
    "failoverAddresses": {
        "scopingTags": {
            "f5_cloud_failover_label": "${cf_label}"
        }
    },
    "failoverRoutes": {
        "scopingTags": {
            "f5_cloud_failover_label": "{cf_label}}"
        },
        "scopingAddressRanges": [
            {
                "range": "${cf_cidr}"
            }
        ],
        "defaultNextHopAddresses": {
            "discoveryType": "static",
            "items": [
                "${az1_nexthop}",
                "${az2_nexthop}"
            ]
        }
    }
}
