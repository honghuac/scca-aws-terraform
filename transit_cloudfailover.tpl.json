{
    "class": "Cloud_Failover",
    "environment": "aws",
    "internalStorage": {
        "scopingTags": {
            "f5_cloud_failover_label": "${cf_label}"
        }
    },
    "failoverAddresses": {
        "scopingTags": {
            "f5_cloud_failover_label": "${cf_label}"
        }
    },
    "failoverRoutes": {
        "scopingTags": {
            "f5_cloud_failover_label": "${cf_label}"
        },
        "scopingAddressRanges": [
            {
                "range": "${cf_cidr1}"
            },
            {
                "range": "${cf_cidr2}"
            },
            {
                "range": "${cf_cidr3}"
            },
            {
                "range": "${cf_cidr4}"
            }
        ],
        "defaultNintHopAddresses": {
            "discoveryType": "static",
            "items": [
                "${cf_nexthop1}",
                "${cf_nexthop2}",
                "${cf_nexthop3}",
                "${cf_nexthop4}"
            ]
        },
        "controls": {
            "class": "Controls",
            "logLevel": "silly"
        }
    }
}